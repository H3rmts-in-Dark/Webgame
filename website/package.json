{
	"name": "website",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"theme": "smui-theme compile src/css/smui.css -i src/css",
		"wasm": "wasm-pack build ./webassembly --out-dir ../src/wasm --target web --dev",
		"wasm build": "wasm-pack build ./webassembly --out-dir ../src/wasm --target web"
	},
	"devDependencies": {
		"@mdi/js": "^7.2.96",
		"@smui/button": "^7.0.0-beta.8",
		"@smui/checkbox": "^7.0.0-beta.8",
		"@smui/form-field": "^7.0.0-beta.8",
		"@smui/icon-button": "^7.0.0-beta.8",
		"@smui/slider": "^7.0.0-beta.8",
		"@smui/textfield": "^7.0.0-beta.8",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"smui-theme": "^6.2.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module",
	"dependencies": {
		"wasm-pack": "^0.11.1"
	}
}
